{
  "expo": {
    "name": "PokeDB",
    "slug": "pokedb",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#aaffaa"
    },
    "description": "A project that aims to make pokemondb.net (a useful Pokemon fan site) more accessible on mobile",
    "githubUrl": "https://github.com/Wolven531/PokeDB.git"
  }
}